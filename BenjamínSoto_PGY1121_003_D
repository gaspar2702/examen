def menu():
  compradores = []
  while True:
    print("""Bienvenido a la aplicacion de casa feliz inmoviliara
        para usted y su familia
        porfavor elija la opcion del men√∫ que desea realizar""")
    print("1. comprar departamento")
    print("2. mostrar departamentos disponibles")
    print("3. ver listado de compradores")
    print("4. mostrar ganancias totales")
    print("5. salir del programa")

    try:

      ventastotales = 0
      choice = int(input("porfavor ingrese un valor de 1 a 5: \n"))
      if choice == 1:
        piso = int(input("porfavor escriba el piso donde desea comprar"))
        if piso == 1:
          depa1 = str(input("ingrese la clase de departamento"))
          if depa1 == "A" or depa1 == "a":
            if departamento1[0] == "X":
              print("departamento ya comprado")
              print(departamento1)
            else:
              print("departamento comprado con exito")
              departamento1.pop(0)
              departamento1.insert(0, "X")
              print(departamento1)
              ventastotales = ventastotales + 38000
              comprador = int(
                  input("ingrese su rut sin punto ni digito berificador"))
              compradores.append(comprador)
          elif depa1 == "B" or depa1 == "b":
            if departamento1[1] == "X":
              print("departamento ya comprado")
              print(departamento1)
            else:
              print("departamento comprado con exito")
              departamento1.pop(1)
              departamento1.insert(1, "X")
              ventastotales = ventastotales + 3000
              comprador = int(
                  input("ingrese su rut sin punto ni digito berificador"))
              compradores.append(comprador)
          elif depa1 == "C" or depa1 == "c":
            if departamento1[2] == "X":
              print("departamento ya comprado")
              print(departamento1)
            else:
              print("departamento comprado con exito")
              departamento1.pop(2)
              departamento1.insert(2, "X")
              print(departamento1)
              ventastotales = ventastotales + 2800
              comprador = int(
                  input("ingrese su rut sin punto ni digito berificador"))
              compradores.append(comprador)
          elif depa1 == "D" or depa1 == "d":
            if departamento1[3] == "X":
              print("departamento ya comprado")
              print(departamento1)
            else:
              print("departamento comprado con exito")
              departamento1.pop(3)
              departamento1.insert(3, "X")
              print(departamento1)
              ventastotales = ventastotales + 3500
              comprador = int(
                  input("ingrese su rut sin punto ni digito berificador"))
              compradores.append(comprador)
